<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Default Search Engine</ShortName>
  <Description>Default-search-engine</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <IconURL>resource://search-plugins/images/yahoo.ico</IconURL>
  <Url type="application/x-suggestions+json" method="GET" template="http://api.bing.com/osjson.aspx" >
    <param name="query" value="{searchTerms}" />
    <param name="form" value="OSDJAS" />
  </Url>
  <Url type="text/html" method="GET" template="https://securesearch.org">
    <Param name="pId" value="BT171101"/>
    <Param name="iDate" value="2019-06-29 10:44:37"/>
    <Param name="bName" value=""/>
    <Param name="sp" value="2"/>
    <Param name="q" value="{searchTerms}"/>
  </Url>
</SearchPlugin>